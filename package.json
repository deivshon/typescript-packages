{
    "scripts": {
        "build": "pnpm -C ./scripts/build execute",
        "dev": "pnpm build && pnpm -r --parallel --filter \"./packages/*\" dev",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "clean": "rm -rf ./packages/*/dist ./scripts/*/dist ./configs/*/dist ./test/*/dist ./test/*/.next",
        "purge": "pnpm clean && rm -rf node_modules ./packages/*/node_modules ./scripts/*/node_modules ./configs/*/node_modules ./test/*/node_modules"
    },
    "devDependencies": {
        "@eslint/js": "^9.30.1",
        "eslint": "^9.30.1",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "globals": "^16.3.0",
        "prettier": "^3.6.2",
        "tsdown": "^0.14.1",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.35.1"
    }
}
