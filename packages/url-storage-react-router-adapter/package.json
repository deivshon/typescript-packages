    "version": "0.0.0",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "build": "tsdown",
        "dev": "tsdown --watch",
        "typecheck": "tsc --noEmit",
        "typecheck:watch": "tsc --noEmit --watch",
        "lint": "eslint .",
        "attw": "attw -qP ."
    },
